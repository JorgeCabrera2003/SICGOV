{
    "name": "jorge/good-vibes",
    "description": "Sistema MVC para la gestion de un restaurante",
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "App\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "require": {
        "php": "^8.1",
        "dompdf/dompdf": "^3.0",
        "phpmailer/phpmailer": "^6.9",
        "vlucas/phpdotenv": "^5.6"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23"
    },
    "config": {
        "optimize-autoloader": true
    }
}